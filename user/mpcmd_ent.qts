set proc nolim

ACCESS *W_PRD LINK "2000", PRDCLE                 &
                TO CIECLE, PRDCLE OF ARCMD_DETAIL &
           LINK TO ARCOMMANDE

SELECT IF CMDDAT <= 19990828

SUBFILE W_CMDENT keep INCLUDE ARCMD_DETAIL
SUBFILE W_CMDDET keep INCLUDE ARCOMMANDE

OUTPUT ARCOMMANDE UPDATE
  ITEM ENTCLE OF ARCOMMANDE FINAL "10" IF PRDANCNUM[6:1] = "1" ELSE "11"

