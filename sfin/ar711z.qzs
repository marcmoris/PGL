USE us900za NOLIST

BUILD ar711za

ACCESS *WAR711PR2

SORT ON CIECLE    OF WAR711PR2 &
     ON DECNUMINV OF WAR711PR2 

DEFINE DZ_NUMRAP  CHARACTER SIZE 08 = "AR711B"
