 USE ussetqzs NOLIST   ; permet de faire les SET REPORT DEV....
 SET NOBLANK           ; Empêche l'impression de ligne blanche


;==============================================================================
; Grand-Livre par compte par unité adm. (par compagnie - par holding)
; prendre note que GL304B est une copie de GL304A la difféerence est
; seulement le tri.
;==============================================================================


ACCESS *WGL304B &

  LINK CIECLE OF WGL304B &
    TO CIECLE OF MCCOMPAGNIE OPTIONAL &

  LINK CIECLE OF WGL304B , &
       UNACLE OF WGL304B   &
    TO CIECLE                 , &
       UNACLE OF MCUNITE_ADM  OPTIONAL &

  LINK REFCIECLE OF WGL304B , &
       REFCLETYP OF WGL304B , &
       REFCLENUM OF WGL304B   &
    TO REFCIECLE , &
       REFCLETYP , &
       REFCLENUM            OF MCREFERENCE OPTIONAL


;MIT EN COMMENTAIRE LE 8-11-99
;ATTENDRE LA SUITE
;SELECT WGL304B IF D_TYPTRS OF WGL304B <> 'ARRC' AND D_TYPTRS OF WGL304B <> 'CPFR' ; DEMANDÉ PAR LE CLIENT

 DEFINE DZ_NOMRAP1 CHARACTER SIZE 60 = &        ; titre1  du rapport
@IF FRANCAIS
                "Grand livre par compte par unité administrative"
@ELSE
                "General Ledger by Account by Unit"
@ENDIF
 DEFINE DZ_NOMRAP2 CHARACTER SIZE 60 = " "      ; titre2  du rapport
 DEFINE DZ_NUMRAP  CHARACTER SIZE 12 = "GL304A"                    ; numéro du rapport
 DEFINE DZ_CIENOM  CHARACTER SIZE 40 = CENTER(CIENOM OF MCCOMPAGNIE)[1:40]; Nom de la compagnie



DEFINE D_ANNUL_LIGN VARCHAR SIZE 181 = &                          ;Pour empêcher l'impression de certaine ligne
"                                                                  " + &
"                                                                  " + &
"                                               "  &
 IF T_FLGDETHIS OF WGL304B = "0" ELSE ""

DEFINE D_SOLDE    FLOAT SIZE 8 &                                  ; Pour calculer la variation
       = HECMNTDBT OF WGL304B - HECMNTCRT OF WGL304B

;
; Losque la description général est absente on imprime le nom de la référence
;
DEFINE D_HISDSCGEN CHARACTER SIZE 40 = REFNOM    OF MCREFERENCE  & ; MP-00-03-14_S22.
                                    IF HISDSCGEN OF WGL304B = "" &
                                  ELSE HISDSCGEN OF WGL304B


DEFINE D_REFCLENUM CHAR SIZE 6 = REFCLENUM OF WGL304B

; Ajustement pour le changement de siècle
DEFINE D_PECCLE_SIE CHARACTER SIZE 5 = "1" + PECCLE OF WGL304B &
         IF PECCLE OF WGL304B[1:1] < "8" &
         ELSE "0" + PECCLE OF WGL304B

SORT   ON  CIECLE OF WGL304B , &                       ; Compagnie
           CPTCLE OF WGL304B , &                       ; Compte
           UNACLE OF WGL304B , &                       ; Unité
           D_PECCLE_SIE , &                       ; Période
;           D_REFCLENUM , &
           HISDAT OF WGL304B , &                       ; Date de transaction
           HISNUMLOT OF WGL304B, &                     ; No Lot
           D_TYPTRS  OF WGL304B;,&                        ; Type de transaction
;           HISNUMDOC OF WGL304B



USE ustit180 NOLIST  ; Titre standard pour l'application
SKIP 2 &
@IF FRANCAIS
 TAB 001 "Période Début : "  &
 TAB 017  G_SELANNFIN OF WGL304B PICTURE "^^" &
 TAB 019 "-" &
 TAB 020  G_SELPECDEB OF WGL304B PICTURE "^^" &
SKIP 1 &
 TAB 001 "Période Fin   : "  &
 TAB 017  G_SELANNFIN OF WGL304B PICTURE "^^" &
 TAB 019 "-" &
 TAB 020  G_SELPECFIN OF WGL304B PICTURE "^^" &
SKIP 2 &
@IF FRANCAIS
 TAB 001 "                  Compte                           Unité administrative     " &
@ELSE
 TAB 001 "%%%               Compte                           Unité administrative     " &
@ENDIF
SKIP &
 TAB 001 "------------------------------------------    ----------------------------- " &
SKIP 1




HEADING AT UNACLE &
        TAB 001 CPTCLE    OF WGL304B   &
        TAB 008 CPTDSC    OF WGL304B   &
        TAB 047 UNACLE    OF WGL304B   &
        TAB 056 UNANOMABR OF MCUNITE_ADM &
@IF FRANCAIS
        TAB 140 "Solde de début....:" &
@ELSE
        TAB 140 "Starting Balance..:" &
@ENDIF
        TAB 161 T_OUV_SANCUMANN_DC OF WGL304B &
        PICTURE " ^^^,^^^,^^^.^^ "  TRAILING SIGN ">" LEADING SIGN "<" SIGNIFICANCE 5 &
        SKIP 2 &
        RESERVE 8 LINES




REPORT &
@IF FRANCAIS
    TAB 008 "Période   Lot      Date     Type      Document  "   PRINT AT UNACLE &
    TAB 061 "              Référence            "                PRINT AT UNACLE &
    TAB 111 "      Débit       "                                 PRINT AT UNACLE & ; MP-01-02-06_D126.
    TAB 131 "     Crédit       "                                 PRINT AT UNACLE & ; MP-01-02-06_D126.
    TAB 151 "      Variation     "                               PRINT AT UNACLE & ; MP-01-02-06_D126.
@ELSE
    TAB 008 "%%%iode   Lot      Date     Type      Document  "   PRINT AT UNACLE &
    TAB 061 "              Référence            "                PRINT AT UNACLE &
    TAB 111 "      Débit       "                                 PRINT AT UNACLE & ; MP-01-02-06_D126.
    TAB 131 "     Crédit       "                                 PRINT AT UNACLE & ; MP-01-02-06_D126.
    TAB 151 "      Variation     "                               PRINT AT UNACLE & ; MP-01-02-06_D126.
@ENDIF
    TAB 001 D_ANNUL_LIGN  &
  SKIP 1 &
    TAB 008 "-------   ----   --------   ----   ---------------" PRINT AT UNACLE &
    TAB 061 "-----------------------------------------------"    PRINT AT UNACLE &
    TAB 111 "------------------"                                 PRINT AT UNACLE & ; MP-01-02-06_D126.
    TAB 131 "------------------"                                 PRINT AT UNACLE & ; MP-01-02-06_D126.
    TAB 151 "--------------------"                               PRINT AT UNACLE & ; MP-01-02-06_D126.
    TAB 001 D_ANNUL_LIGN  &
  SKIP 1 &
    TAB 009 PECCLE    OF WGL304B &
    TAB 018 HISNUMLOT OF WGL304B &
    TAB 025 HISDAT    OF WGL304B &
    TAB 036 D_TYPTRS  OF WGL304B &
    TAB 043 HISNUMDOC OF WGL304B &
    TAB 061 REFCLENUM OF WGL304B &
    TAB 068 D_HISDSCGEN          &
    TAB 111 HECMNTDBT OF WGL304B  PICTURE "^^^,^^^,^^^,^^^.^^" BWZ & ; MP-01-02-06_D126.
    TAB 131 HECMNTCRT OF WGL304B  PICTURE "^^^,^^^,^^^,^^^.^^" BWZ & ; MP-01-02-06_D126.
    TAB 001 D_ANNUL_LIGN   &
  SKIP &
    TAB 61 HISDSCSAI OF WGL304B  &
  SKIP &
    TAB 61 HISDSCSAI2 OF WGL304B


;FOOTING AT D_REFCLENUM  &
;        SKIP 1 &
;        TAB 079 "Total du fournisseur" &
;        TAB 111 HECMNTDBT OF WGL304B SUBTOTAL BWZ          &
;             PICTURE "^^,^^^,^^^.^^" SIGNIFICANCE 4  &
