;/T/ 1.4.5.1 Liste des bons de commande
;
;/P/ Programmeur...: Martin Bruyère
;    Date Création.: 1996-05-31
;
;    Description...: Liste de base des bons de commande
;
;    Paramètres....: Numéro de bon de commande
;
;    Particularités: AUCUNE
;
;-------------------------------------------------------------------------------

USE ussetqzs NOLIST


ACCESS PDBON_COMMANDE &

  LINK CIECLE          OF PDBON_COMMANDE,       &
       BCMNUMBCM       OF PDBON_COMMANDE        &
    TO CIECLE,                                  &
       BCMNUMBCM       OF PDDET_BON_COMMAN      &

  LINK CIECLE          OF PDBON_COMMANDE,       &
       COMNUMCOM       OF PDBON_COMMANDE        &
    TO CIECLE,                                  &
       COMNUMCOM                                OF PDCOMMAN_INTERNE OPTIONAL &

  LINK CIECLE          OF PDBON_COMMANDE,       &
       PJTNUMPRO       OF PDBON_COMMANDE        &
    TO CIECLE,                                  &
       PJTNUMPRO                                OF PDPROJET         OPTIONAL  &

  LINK FOUCLE          OF PDBON_COMMANDE,       &
       FOCCIECLE       OF PDBON_COMMANDE        &
    TO FOUCLE,                                  &
       FOCCIECLE                                OF VFOC_FOU         OPTIONAL  &

  LINK DEVCLE          OF VFOC_FOU              &
    TO DEVCLE                                   OF MCDEVISE         OPTIONAL  &

  LINK FOUCIECLE       OF VFOC_FOU,             &
       FOUREFTYP       OF VFOC_FOU,             &
       FOUCLE          OF VFOC_FOU,             &
       FOCADRACH       OF VFOC_FOU              &
    TO REFCIECLE,                               &
       REFCLETYP,                               &
       REFCLENUM,                               &
       RADCLE                                   OF MCREF_ADRESSE OPTIONAL &

  LINK CIECLE           OF PDBON_COMMANDE       &
    TO CIECLE                                   OF MCCOMPAGNIE &

  LINK TERCLE           OF PDBON_COMMANDE       &
    TO TERCLE                                   OF CPTERME OPTIONAL &

  LINK ("BCMFRSCOU",                            &
       BCMFRSCOU OF PDBON_COMMANDE)             &
    TO XELNOM,                                  &
       CBICLE                                   OF MCCODE_BILINGUE ALIAS A_FRSCOU OPTIONAL &

  LINK ("BCMFRSTRPTER",                         &
       BCMFRSTRPTER OF PDBON_COMMANDE)          &
    TO XELNOM,                                  &
       CBICLE                                   OF MCCODE_BILINGUE ALIAS A_FRSTRPTER OPTIONAL &

  LINK ("BCMFRSTRPMAR",                         &
       BCMFRSTRPMAR OF PDBON_COMMANDE)          &
    TO XELNOM,                                  &
       CBICLE                                   OF MCCODE_BILINGUE ALIAS  A_FRSTRPMAR OPTIONAL &

  LINK ("F",                                    &
       BCMTAXCODTPS)                            &
    TO TAXCLETYP,                               &
       TAXCLECOD                                OF MCTAXE ALIAS A_TPS OPTIONAL &

  LINK ("P",                                    &
       BCMTAXCODTVQ)                            &
    TO TAXCLETYP,                               &
       TAXCLECOD                                OF MCTAXE ALIAS A_TVP OPTIONAL &

  LINK CIECLE OF PDDET_BON_COMMAN,TGRCODGRA OF PDDET_BON_COMMAN            &
    TO CIECLE,TGRCODGRA                         OF PDTYPE_GRANIT 


CHOOSE CIECLE PARM , BCMNUMBCM PARM


SELECT PDBON_COMMANDE IF BCMCODTYPBCM OF PDBON_COMMANDE = "A"


DEFINE D_BCMNUMBCM     CHARACTER SIZE 09 = ASCII(BCMNUMBCM OF PDBON_COMMANDE,9)

DEFINE D_APPROUVE      CHARACTER SIZE 45 = "Approuvé par: _______________________________" IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Approved by: ________________________________"

DEFINE D_COMMANDE_PAR  CHARACTER SIZE 40 = "Commandé par: " + BCMCMDPAR OF PDBON_COMMANDE IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Ordered by : "  + BCMCMDPAR OF PDBON_COMMANDE 

DEFINE D_NO_COMMANDE   CHARACTER SIZE 18 = "NO. DE COMMANDE : " IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "PURCHASE ORDER #: "

; --------------------------------

DEFINE D_LIBEL_CLI     CHARACTER SIZE 25 = "Adressez votre facture à:"   IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Send your invoice to:"     

DEFINE D_LIBEL_FOU     CHARACTER SIZE 23 = "Adresse du fournisseur:"     IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Supplier address:"       

DEFINE D_LIBEL_DEV     CHARACTER SIZE 15 = "Devise........:"             IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Currency......:"     

DEFINE D_DEVDSC        CHARACTER SIZE 15 = DEVDSCFRA OF MCDEVISE         IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE DEVDSCANG OF MCDEVISE

DEFINE D_LIBEL_REF     CHARACTER SIZE 15 = "Référence.....:"             IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Reference.....:"     


DEFINE D_NO_TAXE_PRO   CHARACTER SIZE 25 = "# Taxe provinciale......: "  IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Provincial sales tax no.:"     

DEFINE D_NO_TAXE_FED   CHARACTER SIZE 25 = "# Taxe fédérale.........:"   IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Federal sales tax no....:"     

DEFINE D_CODUNM_X      CHARACTER SIZE 02 = "M3" IF PODCODUNM OF PDDET_BON_COMMAN = " " AND TPDTYPPRD OF PDDET_BON_COMMAN = "BL" &
                                      ELSE "M2" IF PODCODUNM OF PDDET_BON_COMMAN = " " AND TPDTYPPRD OF PDDET_BON_COMMAN = "TR" &
                                      ELSE PODCODUNM OF PDDET_BON_COMMAN

DEFINE D_CODUNM        CHARACTER SIZE 02 = D_CODUNM_X IF FOULNG OF VFOC_FOU = "F"  &
                                      ELSE "CM"       IF D_CODUNM_X         = "M3" &
                                      ELSE "SM"       IF D_CODUNM_X         = "M2" &
                                      ELSE "CF"       IF D_CODUNM_X         = "P3" &
                                      ELSE "SF"       IF D_CODUNM_X         = "P2" 

DEFINE D_MSG_01        CHARACTER SIZE 132 = "1-Veuillez accuser réception de cette demande en confirmant les prix et la date de livraison"                                        IF FOULNG OF VFOC_FOU = "F" &
                                       ELSE "1-Acknowledge receipt of this order confirming prices and delivery date."
DEFINE D_MSG_02        CHARACTER SIZE 132 = "2-Nous n'acceptons aucun changement et aucune substitution sans autorisation."                                                       IF FOULNG OF VFOC_FOU = "F" &
                                       ELSE "2-No substitutions or changes will be accepted without a written authorization."
DEFINE D_MSG_03        CHARACTER SIZE 132 = "3-Nous réservons le droit d'annuler cette commande si l'expédition n'est pas conforme pour les quantités, prix ou date de livraison" IF FOULNG OF VFOC_FOU = "F" &
                                       ELSE "3-We reserve the right to cancel this order if shipment is not made in quantities, prices and delivery shown."
DEFINE D_MSG_04        CHARACTER SIZE 132 = "4-Les numéros de licence pour la taxe de vente doivent s'appliquer lorsqu'ils sont indiqués."                                        IF FOULNG OF VFOC_FOU = "F" &
                                       ELSE "4-Sales taxe licence numbers are applicable when shown."


DEFINE D_PODLONG       CHARACTER SIZE 10 = CENTER(PODLONG OF PDDET_BON_COMMAN) IF PODCODUNM OF PDDET_BON_COMMAN = "P2" OR PODCODUNM OF PDDET_BON_COMMAN = "P3"
DEFINE D_PODHAUT       CHARACTER SIZE 10 = CENTER(PODHAUT OF PDDET_BON_COMMAN) IF PODCODUNM OF PDDET_BON_COMMAN = "P2" OR PODCODUNM OF PDDET_BON_COMMAN = "P3"
DEFINE D_PODLARG       CHARACTER SIZE 10 = CENTER(PODLARG OF PDDET_BON_COMMAN) IF PODCODUNM OF PDDET_BON_COMMAN = "P2" OR PODCODUNM OF PDDET_BON_COMMAN = "P3"

DEFINE D_PODLON        INTEGER   SIZE 04 = PODLON OF PDDET_BON_COMMAN          IF PODCODUNM OF PDDET_BON_COMMAN = "M2" OR PODCODUNM OF PDDET_BON_COMMAN = "M3" OR PODCODUNM OF PDDET_BON_COMMAN = " "
DEFINE D_PODHAU        INTEGER   SIZE 04 = PODHAU OF PDDET_BON_COMMAN          IF PODCODUNM OF PDDET_BON_COMMAN = "M2" OR PODCODUNM OF PDDET_BON_COMMAN = "M3" OR PODCODUNM OF PDDET_BON_COMMAN = " "
DEFINE D_PODEPA        INTEGER   SIZE 04 = PODEPA OF PDDET_BON_COMMAN          IF PODCODUNM OF PDDET_BON_COMMAN = "M2" OR PODCODUNM OF PDDET_BON_COMMAN = "M3" OR PODCODUNM OF PDDET_BON_COMMAN = " "

; --------------------------------

DEFINE D_LIBEL_TELGRA  CHARACTER SIZE 15 = "Téléphone.....:" IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Phone.........:" 
                                                             
DEFINE D_LIBEL_TELFOU  CHARACTER SIZE 15 = "Téléphone.....:" IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE "Phone.........:" 
                                                             
DEFINE D_LIBEL_FAXGRA  CHARACTER SIZE 15 = "Télécopieur...:" IF "F" = FOULNG OF VFOC_FOU &
