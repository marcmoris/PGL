DEFINE D_TERME         CHARACTER SIZE 40 = TERDSCFRA OF CPTERME   IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE TERDSCANG OF CPTERME                     
                                                                                    
DEFINE D_FRSCOU        CHARACTER SIZE 40 = CBIDSCFRA OF A_FRSCOU  IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE CBIDSCANG OF A_FRSCOU                    
                                                                                    
DEFINE D_FRSTRPTER     CHARACTER SIZE 40 = CBIDSCFRA OF A_FRSTRPTER IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE CBIDSCANG OF A_FRSTRPTER                 
                                                                                    
DEFINE D_FRSTRPMAR     CHARACTER SIZE 40 = CBIDSCFRA OF A_FRSTRPMAR IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE CBIDSCANG OF A_FRSTRPMAR                 
                                                                                    
DEFINE D_DSCTAXTPS     CHARACTER SIZE 40 = TAXDSCFRA OF A_TPS IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE TAXDSCANG OF A_TPS 
                                                              
DEFINE D_DSCTAXTVP     CHARACTER SIZE 40 = TAXDSCFRA OF A_TVP IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE TAXDSCANG OF A_TVP 
                                                                                    
DEFINE D_TGRDSCFRA001  CHARACTER SIZE 30 = TGRDSCFRA001 OF PDTYPE_GRANIT IF "F" = FOULNG OF VFOC_FOU &
                                      ELSE TGRDSCANG001 OF PDTYPE_GRANIT 
                                                                         
DEFINE D_QTECMD        FLOAT     SIZE 08 = ROUND((PODVOL OF PDDET_BON_COMMAN * PODQTECMD OF PDDET_BON_COMMAN),4) IF PODCODUNM OF PDDET_BON_COMMAN = "P3" OR &
                                                                                                                    PODCODUNM OF PDDET_BON_COMMAN = "P2"    &
                                      ELSE PODVOLCMD OF PDDET_BON_COMMAN                                         IF TPDTYPPRD OF PDDET_BON_COMMAN = "BL"    &
                                      ELSE PODSURCMD OF PDDET_BON_COMMAN


; --------------------------------

DEFINE D_MNTCMD        FLOAT     SIZE 08 = ROUND(PODVOLCMD OF PDDET_BON_COMMAN * PODPRI OF PDDET_BON_COMMAN,0)                                IF  TPDTYPPRD OF PDDET_BON_COMMAN = "BL"  AND &
                                                                                                                                                 (PODCODUNM OF PDDET_BON_COMMAN = "M3"   OR &
                                                                                                                                                  PODCODUNM OF PDDET_BON_COMMAN = "  ")     &
                                      ELSE ROUND(PODSURCMD OF PDDET_BON_COMMAN * PODPRI OF PDDET_BON_COMMAN,0)                                IF  TPDTYPPRD OF PDDET_BON_COMMAN = "TR"  AND &
                                                                                                                                                 (PODCODUNM OF PDDET_BON_COMMAN = "M2"   OR &
                                                                                                                                                  PODCODUNM OF PDDET_BON_COMMAN = "  ")     &
                                      ELSE ROUND(PODQTECMD OF PDDET_BON_COMMAN * (PODVOL OF PDDET_BON_COMMAN * PODPRI OF PDDET_BON_COMMAN),0) IF (TPDTYPPRD OF PDDET_BON_COMMAN = "BL"  AND &
                                                                                                                                                  PODCODUNM OF PDDET_BON_COMMAN = "P3")  OR &
                                                                                                                                                 (TPDTYPPRD OF PDDET_BON_COMMAN = "TR"  AND &
                                                                                                                                                  PODCODUNM OF PDDET_BON_COMMAN = "P2") 
; --------------------------------


DEFINE D_TITRE      CHARACTER SIZE 130 =  &
"Type         Type de granit      Fini    # Bloc    Longueur    Hauteur    Largeur  Quantité     Prix unitaire      Prix du bloc  " IF TPDTYPPRD OF PDDET_BON_COMMAN = "BL" AND "F" = FOULNG OF VFOC_FOU ELSE &
"Type          Granite type      Finish  Block no.   Lenght     Height      Width   Quantity      Unit price         Block price  " IF TPDTYPPRD OF PDDET_BON_COMMAN = "BL" AND "A" = FOULNG OF VFOC_FOU ELSE &
"Type         Type de granit      Fini    # Bloc    Longueur    Hauteur  Thickness  Quantité     Prix unitaire      Prix du bloc  " IF TPDTYPPRD OF PDDET_BON_COMMAN <>"BL" AND "F" = FOULNG OF VFOC_FOU ELSE &
"Type          Granite type      Finish  Block no.   Lenght     Height   Épaisseur  Quantity      Unit price         Block price  " IF TPDTYPPRD OF PDDET_BON_COMMAN <>"BL" 
 

DEFINE D_SOUS_LIGNE CHARACTER SIZE 130 = &
"----- ------------------------- ------ ---------- ---------- ---------- ---------- ---------    -------------     ---------------"


