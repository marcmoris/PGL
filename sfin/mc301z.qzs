@ELSE
 TAB 002 "%%%" &
@ENDIF
 TAB 018 CIENOMABR OF MCCOMPAGNIE &
 TAB 082 CIEADR2 OF MCCOMPAGNIE &
SKIP &
 TAB 082 CIEADR3 OF MCCOMPAGNIE &
SKIP &
@IF FRANCAIS
 TAB 002 "Téléphone.....:" &
@ELSE
 TAB 002 "%%%" &
@ENDIF
 TAB 018 CIETEL  OF MCCOMPAGNIE &
 TAB 082 CIEADR4 OF MCCOMPAGNIE &
SKIP &
@IF FRANCAIS
 TAB 002 "Numéro du FAX.:" &
 TAB 018 CIEFAX  OF MCCOMPAGNIE &
@ENDIF
@IF FRANCAIS
 TAB 037 "Télex.........:" &
@ELSE
 TAB 037 "%%%" &
@ENDIF
 TAB 053 CIETLX  OF MCCOMPAGNIE &
@IF FRANCAIS
 TAB 066 "Code postal...:" &
@ELSE
 TAB 066 "%%%" &
@ENDIF
 TAB 082 CIECP   OF MCCOMPAGNIE &
SKIP 2 &
@IF FRANCAIS
 TAB 002 "Enreg. TPS....:" &
@ELSE
 TAB 002 "%%%" &
@ENDIF
 TAB 018 CIEENRTPS OF MCCOMPAGNIE &
@IF FRANCAIS
 TAB 037 "Compte avance.:" &
@ELSE
 TAB 037 "%%%" &
@ENDIF
 TAB 053 CIECPTAVA OF MCCOMPAGNIE &
 TAB 061 CPTDSC OF VCPT_CPTAVA &
SKIP &
@IF FRANCAIS
 TAB 002 "Enreg. TVP....:" &
@ELSE
 TAB 002 "%%%" &
@ENDIF
 TAB 018 CIEENRTVQ OF MCCOMPAGNIE &
@IF FRANCAIS
 TAB 037 "Cpt ava. due..:" &
@ELSE
 TAB 037 "%%%" &
@ENDIF
 TAB 053 CIECPTDUE OF MCCOMPAGNIE &
 TAB 061 CPTDSC OF VCPT_CPTDUE &
SKIP &
@IF FRANCAIS
 TAB 037 "Unité de bilan:" &
@ELSE
 TAB 037 "%%%" &
@ENDIF
 TAB 053 CIEUNABIL  OF MCCOMPAGNIE &
 TAB 061 UNANOM     OF MCUNITE_ADM &
SKIP &
 TAB 001 "-----------------------------------------------------------------------------------------------------------"&
 TAB 108 "-------------------------" &
SKIP 2 &
RESERVE 13 LINES

BUILD mc301z

