USE ussetqzs NOLIST

ACCESS MCCOMPAGNIE &
  LINK CIECPTAVA OF MCCOMPAGNIE                         &
    TO CPTCLE    OF VCPT_DSC ALIAS VCPT_CPTAVA OPTIONAL &
  LINK CIECPTDUE OF MCCOMPAGNIE                         &
    TO CPTCLE    OF VCPT_DSC ALIAS VCPT_CPTDUE OPTIONAL &
  LINK CIECLE OF MCCOMPAGNIE                          , &
       CIEUNABIL OF MCCOMPAGNIE                         &
    TO CIECLE                                         , &
       UNACLE    OF MCUNITE_ADM OPTIONAL


 DEFINE DZ_NOMRAP1 CHARACTER SIZE 60 = &        ; titre1  du rapport
@IF FRANCAIS
                "Liste des compagnies"
@ELSE
                "%%%Liste des compagnies"
@ENDIF
 DEFINE DZ_NOMRAP2 CHARACTER SIZE 60 = " "      ; titre2  du rapport
 DEFINE DZ_NUMRAP  CHARACTER SIZE 12 = "MC301"  ; numéro du rapport
 DEFINE DZ_CIENOM  CHARACTER SIZE 40 = PARM     ; Nom de la compagnie


SORT   ON  CIECLE OF MCCOMPAGNIE                ; Code de Compagnie


USE ustitstd NOLIST  ; Titre standard pour l'application
SKIP 2


REPORT &
@IF FRANCAIS
 TAB 002 "Compagnie.....:" &
@ELSE
 TAB 002 "%%%" &
@ENDIF
 TAB 018 CIECLE OF MCCOMPAGNIE &
@IF FRANCAIS
 TAB 042 "Nomenclature..:" &
@ELSE
 TAB 032 "%%%" &
@ENDIF
 TAB 058 CIENMC OF MCCOMPAGNIE &
SKIP 2 &
@IF FRANCAIS
 TAB 002 "Nom compagnie.:" &
@ELSE
 TAB 002 "%%%" &
@ENDIF
 TAB 018 CIENOM OF MCCOMPAGNIE &
@IF FRANCAIS
 TAB 066 "Adresse.......:" &
@ELSE
 TAB 066 "%%%" &
@ENDIF
 TAB 082 CIEADR1 OF MCCOMPAGNIE &
SKIP &
@IF FRANCAIS
