USE ussetqzs NOLIST   ; permet de faire les SET REPORT DEV....
SET NOBLANK           ; Empêche l'impression de ligne blanche


ACCESS *WCP307 &
  LINK REFCIECLE OF WCP307, &
       REFCLETYP OF WCP307, &
       REFCLENUM OF WCP307  &
    TO REFCIECLE          , &
       REFCLETYP          , &
       REFCLENUM          OF MCREFERENCE OPTIONAL &
  LINK DEVCLE OF MCREFERENCE &
    TO DEVCLE             OF MCDEVISE OPTIONAL


 DEFINE DZ_NOMRAP1 CHARACTER SIZE 60 = & ; titre1  du rapport
@IF FRANCAIS
                   "Validation des factures/Notes de crédit/Crédits/Ajustements"
@ELSE
                   "%%%Validation des factures/Notes de crédit/Crédits/Ajustements"
@ENDIF
 DEFINE DZ_NOMRAP2 CHARACTER SIZE 60 = "Sommaire" ; titre2  du rapport

 DEFINE DZ_NUMRAP  CHARACTER SIZE 12 = "CP307ZB"       ; numéro du rapport
 DEFINE DZ_CIENOM  CHARACTER SIZE 40 = PARM           ; nom de la compagnie



DEFINE D_MNTCAN FLOAT SIZE 8 = ROUND(CAPMNTNET OF WCP307 * DEVTAU OF MCDEVISE)
DEFINE D_MNTCTI FLOAT SIZE 8 = ROUND(CAPMNTCTI OF WCP307 * DEVTAU OF MCDEVISE)
DEFINE D_MNTRTI FLOAT SIZE 8 = ROUND(CAPMNTRTI OF WCP307 * DEVTAU OF MCDEVISE)

; Ajustement pour le changement de siècle
DEFINE D_PECCLE_WCP307 CHARACTER SIZE 5 = "1" + PECCLE OF WCP307 &
         IF PECCLE OF WCP307[1:1] < "8" &
         ELSE "0" + PECCLE OF WCP307

SORT ON D_PECCLE_WCP307 &
     ON LCPCLE    OF WCP307 &          ; No lot
     ON DEVCLE    OF MCREFERENCE &
     ON REFCLENUM OF WCP307 &          ; No Client
     ON RADCLE    OF WCP307 &          ; Numéro d'adresse.
     ON CAPDAT    OF WCP307 &          ; Date de facture
     ON CAPCLE    OF WCP307            ; Numéro de facture


 USE ustitstd NOLIST  ; Titre standard pour l'application
SKIP &
@IF FRANCAIS
 TAB 001 "Période: " &
@ELSE
 TAB 001 "Period.: " &
@ENDIF
 TAB 010 PECCLE OF WCP307 &
SKIP 2 &
  TAB 007 "Lot      Devise        Montant brut                CTI         RTI     Montant net Montant net can nbr" &
SKIP &
  TAB 007 "---- ---------------  ---------------      ----------- ----------- --------------- --------------- ---"  &
SKIP 2


HEADING AT D_PECCLE_WCP307 &
SKIP PAGE

FOOTING AT DEVCLE &
  TAB 007 LCPCLE OF WCP307 &
  TAB 012 DEVDSCFRA OF MCDEVISE PICTURE "^^^^^^^^^^^^^^^" &
  TAB 029 CAPMNT      OF WCP307 SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 050 D_MNTCTI              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^.^^ "     SIGNIFICANCE 4 &
  TAB 062 D_MNTRTI              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^.^^ "     SIGNIFICANCE 4 &
  TAB 074 CAPMNTNET   OF WCP307 SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 090 D_MNTCAN              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 106 COUNT                          AT CAPCLE PICTURE "^^^"


FOOTING AT LCPCLE &
SKIP 2 &
  TAB 029 "---------------      ----------- ----------- ---------------  --------------- ---" &
SKIP &
@IF FRANCAIS
  TAB 001 "Total Lot" LCPCLE OF WCP307 &
@ELSE
  TAB 001 "Batch total" LCPCLE OF WCP307 &
@ENDIF
  TAB 029 CAPMNT      OF WCP307 SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 050 D_MNTCTI              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^.^^ "     SIGNIFICANCE 4 &
  TAB 062 D_MNTRTI              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^.^^ "     SIGNIFICANCE 4 &
  TAB 074 CAPMNTNET   OF WCP307 SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 090 D_MNTCAN              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 106 COUNT                          AT CAPCLE PICTURE "^^^" &
SKIP 3

FINAL FOOTING &
SKIP 2 &
  TAB 029 "---------------      ----------- ----------- ---------------  --------------- ---" &
SKIP &
@IF FRANCAIS
  TAB 001 "Total pour tous les lots" &
@ELSE
  TAB 001 "Batch Total For All" &
@ENDIF
  TAB 029 CAPMNT      OF WCP307 SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 050 D_MNTCTI              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^.^^ "     SIGNIFICANCE 4 &
  TAB 062 D_MNTRTI              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^.^^ "     SIGNIFICANCE 4 &
  TAB 074 CAPMNTNET   OF WCP307 SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 090 D_MNTCAN              SUBTOTAL AT CAPCLE PICTURE "^^^,^^^,^^^.^^ " SIGNIFICANCE 4 &
  TAB 106 COUNT                          AT CAPCLE PICTURE "^^^^" &
SKIP &
