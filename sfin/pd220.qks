;-------------------------------------------------------------------------------
;
; Valide la sécurité d'accès de l'écran.
;

PROCEDURE INITIALIZE
BEGIN
  USE ussececr NOLIST
END


;-------------------------------------------------------------------------------
;
; Permet d'afficher les champs se rapportant à PJTABR
;

PROCEDURE PROCESS PJTABR OF PDCOMMAN_INTERNE
BEGIN
  LET PJTANN     OF PDCOMMAN_INTERNE = PJTANN OF PDPROJET
  DISPLAY PJTANN OF PDCOMMAN_INTERNE
END


;-------------------------------------------------------------------------------
;
; Appel du dépisteur (pop-up) pour le champ.
;    Paramètres...: T_TRANUMTRA002  ==> Numéro de tranche
;                   T_COD           ==> 0 = Tous   ,   1 = (I ou R)
;                                                      Inventaire ou Réservé

PROCEDURE INPUT TRANUMTRA002 OF PDCOMMAN_ASSIGN
BEGIN
  IF 0 <> INDEX(FIELDTEXT,"*")
  THEN BEGIN
    LET T_COD          = 1 ; Inventaire ou Réservé
    LET T_TRANUMTRA002 = FIELDTEXT[1:INDEX(FIELDTEXT,"*") - 1] + "@"
    RUN SCREEN pd122 MODE F &
                     PASSING T_TRANUMTRA002, &
                             T_COD, &
                             T_CIECLEMNU
    LET FIELDTEXT = TRUNCATE( T_TRANUMTRA002 )
  END
END


;-------------------------------------------------------------------------------
;
; Valider les dimensions selon les tolérances permises et le type de granit.
;

PROCEDURE EDIT T_SILENT
BEGIN
IF NOT DELETEDRECORD OF PDCOMMAN_ASSIGN
THEN BEGIN
  ;
  ; Vérifier si une ligne de la commande interne
  ; répond aux valeurs de la tranche assignée
  ; selon le SELECT sur le fichier DESIGNER.
  ; Les variables suivantes prennent les valeurs "0" si FAUX, "1" si VRAI
  ;
  LET T_CASERR   = "0"
  LET T_EPAIS    = "1"
  LET T_GRANIT   = "1"
  LET T_POSSIBLE = "0"
  LET T_TROUVE   = "0"

  WHILE RETRIEVING PDDETAIL_C_I &
     VIA   CIECLE,       &
           PJTABR,       &
           COMNUMCOMINT  &
     USING T_CIECLEMNU,                      &
           PJTABR        OF PDCOMMAN_ASSIGN, &
           COMNUMCOMINT  OF PDCOMMAN_ASSIGN
  BEGIN
     ;
     ; Des tranches ayant une épaisseur utilisable ont été trouvées
     ;
     IF     T_TROUVE = "0" &
       OR &
          ( T_TROUVE = "1" AND T_GRANIT = "0" )
     THEN BEGIN
        ;
        ; Vérifier si entre les bornes permises
        ;
        IF  TRAEPA OF PDTRANCHE <=   DCIEPA OF PDDETAIL_C_I &
                                   + COMEPAPLU OF PDCOMMAN_INTERNE &
          AND &
            TRAEPA OF PDTRANCHE >=   DCIEPA OF PDDETAIL_C_I &
                                  - COMEPAMOI OF PDCOMMAN_INTERNE
        THEN BEGIN
           LET T_TROUVE = "1"
           ;
           ; Vérifier si une possibilité trop épaisse a déjà été trouvée
           ;
           IF T_POSSIBLE = "1"
           THEN BEGIN
              LET T_CASERR   = "0"
              LET T_EPAIS    = "1"
              LET T_GRANIT   = "1"
              LET T_POSSIBLE = "0"
           END

           ;
           ; Vérifier le code de granit
           ;
           IF TGRCODGRA OF PDTRANCHE = TGRCODGRA OF PDDETAIL_C_I
           THEN BEGIN
              LET T_GRANIT = "1"
              BREAK
           END
           ELSE BEGIN ; Code de granit différent
              LET T_CASERR = "1"
              LET T_GRANIT = "0"
           END ; Fin du if code granit
        END
        ELSE BEGIN ; Trop épais
           ;
           ; Évaluer si une possibilité meilleur est possible
           ;
           IF   T_POSSIBLE = "0" &
             OR &
                (T_POSSIBLE = "1" AND T_GRANIT = "0")
           THEN BEGIN
              LET T_CASERR   = "1"
              LET T_EPAIS    = "0"
              LET T_POSSIBLE = "1"
              ;
              ; Vérifier le code de granit
              ;
              IF TGRCODGRA OF PDTRANCHE = TGRCODGRA OF PDDETAIL_C_I
              THEN BEGIN ; Seul l'épaisseur est en erreur
                 LET T_GRANIT   = "1"
              END
              ELSE BEGIN ; Utilisation possible avec erreurs
                 LET T_GRANIT   = "0"
              END ; Fin du if code granit POSSIBLE
           END ; Fin du if possible
        END ; Fin du if épaisseur
     END ; Fin du IF TROUVE
  END  ; Fin du WHILE RETRIEVING

  ;
  ; Évaluation pour l'affichage des messages d'erreurs.
  ;
  IF T_TROUVE = "1"
  THEN BEGIN
    IF T_GRANIT = "0"
    THEN BEGIN
       WARNING 3121 NOW RESPONSE ;"Le type de granit ne correspond pas à celui commandé."
    END
  END
  ELSE BEGIN
    IF T_POSSIBLE = "1"
    THEN BEGIN
       WARNING 3166 NOW RESPONSE ;"L'épaisseur est supérieure aux valeurs acceptables."

       IF T_GRANIT = "0"
       THEN BEGIN
          WARNING 3121 NOW RESPONSE ; "Le type de granit ne correspond pas à celui commandé."
       END
    END
    ELSE BEGIN
       ERROR 3165 ; "L'épaisseur est inférieure aux valeurs acceptables."
    END
  END ; Fin du if TROUVE
END ; Fin du if not deletedrecord
END


;-------------------------------------------------------------------------------
;
; Procédure pour convertir OUI,NON en 1,0
;       OUI, YES = 1
;       NON      = 0
;

PROCEDURE INPUT CASACCERR OF PDCOMMAN_ASSIGN
BEGIN
  ;
  ; Pour forcer l'utilisation du EDIT du champ
  ;
  IF CASACCERR OF PDCOMMAN_ASSIGN = "0"  ; Valeur par défaut
  THEN BEGIN
    IF FIELDTEXT = ""
    THEN BEGIN
      LET FIELDTEXT = "0"
    END
  END

  USE usflginp NOLIST
END


;-------------------------------------------------------------------------------
;
; Validation des erreurs de tranches
;
PROCEDURE EDIT CASACCERR OF PDCOMMAN_ASSIGN
BEGIN
  IF T_CASERR = "1"
  THEN BEGIN
    IF NOT DELETEDRECORD OF PDCOMMAN_ASSIGN
    THEN BEGIN
      IF FIELDTEXT = "0"
      THEN BEGIN
        LET T_FLG_DEL = "1"
        DELETE PDCOMMAN_ASSIGN
        WARNING 3167 ; Cette tranche en erreur n'a pas été acceptée.
      END
    END
  END
END


;-------------------------------------------------------------------------------
;
; Procédure pour convertir 1,0 en OUI,NON
;
PROCEDURE OUTPUT CASACCERR OF PDCOMMAN_ASSIGN
BEGIN
  USE usflgout NOLIST
END


;-------------------------------------------------------------------------------
;
; Valide si l'usager peut faire de la saisie.
;

PROCEDURE PREENTRY
BEGIN
  USE ussecent NOLIST
END


;-------------------------------------------------------------------------------
;
; Valide les traitements permis par écran et par usager.
; Met le statut de bloc à "A" (assignation) à la création
; et à "I" lors de la destruction d'une assignation
;

PROCEDURE PREUPDATE
BEGIN
  ;
  ; Modification dans PDCOMMAN_ASSIGN et A_TRA_TRANCHE
  ;
  FOR PDCOMMAN_ASSIGN
  BEGIN
    IF T_FLG_DEL = ""
    THEN BEGIN
      ;
      ; Validation de la destruction
      ;
      IF     DELETEDRECORD OF PDCOMMAN_ASSIGN &
         AND TZ_SECDEL = "0"
      THEN ERROR 1
      ;
      ; Validation de la modification.
      ;
      IF     ALTEREDRECORD     OF PDCOMMAN_ASSIGN &
         AND NOT NEWRECORD     OF PDCOMMAN_ASSIGN &
         AND NOT DELETEDRECORD OF PDCOMMAN_ASSIGN &
         AND TZ_SECMOD = "0"
      THEN ERROR 2

      ;
      ; Utiliser A_TRA_TRANCHE pour les mise à jour des informations
      ; le fichier PDTRANCHE étant "REFERENCE"
      ;
      GET A_TRA_TRANCHE &
            VIA   CIECLE,          &
                  TRANUMTRA002     &
            USING T_CIECLEMNU ,    &
                  TRANUMTRA002 OF PDCOMMAN_ASSIGN

      ;
      ;  Historique des modifications du fichier de PDTRANCHE
      ;
      IF ALTEREDRECORD OF PDCOMMAN_ASSIGN
      THEN BEGIN

        LET TRADATMAJ OF A_TRA_TRANCHE = SYSDATE
        LET TRAHREMAJ OF A_TRA_TRANCHE = SYSTIME / 10000
        LET TRAUSRMAJ OF A_TRA_TRANCHE = LOGONID
      END
      ;
      ; Modification du statut de tranche
      ;
      IF  DELETEDRECORD OF PDCOMMAN_ASSIGN
      THEN BEGIN
        LET TRASTU OF A_TRA_TRANCHE = "I"
      END

      IF NEWRECORD OF PDCOMMAN_ASSIGN
      THEN BEGIN
        LET TRASTU OF A_TRA_TRANCHE = "A"
      END
      ;
      ; Répercuter les modifications dans le fichier PDTRANCHE
      ;
      PUT A_TRA_TRANCHE
    END ; Fin du if t_flg_delete
  END ; Fin du FOR
END


BUILD


