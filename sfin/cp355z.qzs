USE ussetqzs NOLIST ; Permet de faire les set report device...
SET NOHEAD
SET FORMFEED
SET PAGE LEN 72 WIDTH 76 IMAGES 2
SET REPORT SPACING 0
SET NOBLANKS

;-------------------------------------------------------------------------------
;
;  IMPRESSION DES CHÈQUES
;
;

ACCESS *WCP355B &
  LINK REFCIECLE, &
       REFCLENUM &
    TO FOUCIECLE, &
       FOUCLE OF CPFOURNISSEUR OPTIONAL


SORT ON PAMCLE &
     ON T_NBRLIG

;***  CODE DE CONTROLE - CADRE ET JEUX DE CARAC.
;***********************************************
DEFINE ESCBACBAS        CHAR*5  = "&l4H"
DEFINE ESCCADRE         CHAR*65 = "%0BIN;SP1;PW0;pu0,3420;ea8128,6450;pu0,7100;ea8128,10160;%0A"
DEFINE ESCLIGNE1        CHAR*45 = "%0BIN;SP1;PW0;PU4050,7400;EA4050,9725;%0A" IF T_TYPCHQ = "1" & ; Ligne de séparation des factures
                             ELSE ""
DEFINE ESCLIGNE2        CHAR*45 = "%0BIN;SP1;PW0;PU4050,3700;EA4050,6015;%0A" IF T_TYPCHQ = "1" & ; Ligne de séparation des factures
                             ELSE ""
DEFINE ESCSPECIAL       CHAR*45 = "*t300R*p1760x2467Y*c475a70b10g2P*p0x0Y" ; fond du mnt ombrager *p1760 = colonne x2467Y = ligne
DEFINE ESCCOND          CHAR*15 = "(10U&l8D&k2S"  ; 16 CPS, 8 ligne/pouce
DEFINE ESCNORM          CHAR*15 = "(10U&l6D&k0S"  ; 10 CPS, 6 ligne/pouce

;***  37.5 = 1 LIGNE  A  8 LIGNE/POUCE (300 DPI)
;***********************************************
DEFINE D-SAUT-TALON1-TOT  CHAR*7 = "*p792Y"  ; montant cheque sur talon 1
DEFINE D-SAUT-TALON2-TOT  CHAR*8 = "*p1878Y" ; montant cheque sur talon 2 - ln 1

DEFINE D-SAUT-TALON2-TET  CHAR*8 = "*p1129Y" ; entete talon 2 - ligne 1


;***  TALON 1 : Position de chaque ligne de facture pour PCL5 de HP
;*******************************************************************
DEFINE D-SAUT-TALON1-FAC CHAR*7 = &
              "*p266Y" IF (1  = MOD(T_NBRLIG,28) OR 2  = MOD(T_NBRLIG,28)) ELSE &
              "*p304Y" IF (3  = MOD(T_NBRLIG,28) OR 4  = MOD(T_NBRLIG,28)) ELSE &
              "*p342Y" IF (5  = MOD(T_NBRLIG,28) OR 6  = MOD(T_NBRLIG,28)) ELSE &
              "*p380Y" IF (7  = MOD(T_NBRLIG,28) OR 8  = MOD(T_NBRLIG,28)) ELSE &
              "*p417Y" IF (9  = MOD(T_NBRLIG,28) OR 10 = MOD(T_NBRLIG,28)) ELSE &
              "*p455Y" IF (11 = MOD(T_NBRLIG,28) OR 12 = MOD(T_NBRLIG,28)) ELSE &
              "*p492Y" IF (13 = MOD(T_NBRLIG,28) OR 14 = MOD(T_NBRLIG,28)) ELSE &
              "*p530Y" IF (15 = MOD(T_NBRLIG,28) OR 16 = MOD(T_NBRLIG,28)) ELSE &
              "*p567Y" IF (17 = MOD(T_NBRLIG,28) OR 18 = MOD(T_NBRLIG,28)) ELSE &
              "*p605Y" IF (19 = MOD(T_NBRLIG,28) OR 20 = MOD(T_NBRLIG,28)) ELSE &
              "*p642Y" IF (21 = MOD(T_NBRLIG,28) OR 22 = MOD(T_NBRLIG,28)) ELSE &
              "*p680Y" IF (23 = MOD(T_NBRLIG,28) OR 24 = MOD(T_NBRLIG,28)) ELSE &
              "*p717Y" IF (25 = MOD(T_NBRLIG,28) OR 26 = MOD(T_NBRLIG,28)) ELSE &
              "*p755Y" IF (27 = MOD(T_NBRLIG,28) OR 0 = MOD(T_NBRLIG,28)) ELSE &
              "*p755Y"

;***  TALON 2 : Position de chaque ligne de facture pour PCL5 de HP
;*******************************************************************
DEFINE D-SAUT-TALON2-FAC CHAR*8 =  &
             "*p1360Y" IF (1  = MOD(T_NBRLIG,28) OR 2  = MOD(T_NBRLIG,28)) ELSE &
             "*p1397Y" IF (3  = MOD(T_NBRLIG,28) OR 4  = MOD(T_NBRLIG,28)) ELSE &
             "*p1435Y" IF (5  = MOD(T_NBRLIG,28) OR 6  = MOD(T_NBRLIG,28)) ELSE &
             "*p1472Y" IF (7  = MOD(T_NBRLIG,28) OR 8  = MOD(T_NBRLIG,28)) ELSE &
             "*p1510Y" IF (9  = MOD(T_NBRLIG,28) OR 10 = MOD(T_NBRLIG,28)) ELSE &
             "*p1547Y" IF (11 = MOD(T_NBRLIG,28) OR 12 = MOD(T_NBRLIG,28)) ELSE &
             "*p1585Y" IF (13 = MOD(T_NBRLIG,28) OR 14 = MOD(T_NBRLIG,28)) ELSE &
             "*p1622Y" IF (15 = MOD(T_NBRLIG,28) OR 16 = MOD(T_NBRLIG,28)) ELSE &
             "*p1660Y" IF (17 = MOD(T_NBRLIG,28) OR 18 = MOD(T_NBRLIG,28)) ELSE &
             "*p1697Y" IF (19 = MOD(T_NBRLIG,28) OR 20 = MOD(T_NBRLIG,28)) ELSE &
             "*p1735Y" IF (21 = MOD(T_NBRLIG,28) OR 22 = MOD(T_NBRLIG,28)) ELSE &
             "*p1772Y" IF (23 = MOD(T_NBRLIG,28) OR 24 = MOD(T_NBRLIG,28)) ELSE &
             "*p1810Y" IF (25 = MOD(T_NBRLIG,28) OR 26 = MOD(T_NBRLIG,28)) ELSE &
             "*p1847Y" IF (27 = MOD(T_NBRLIG,28) OR 0 = MOD(T_NBRLIG,28)) ELSE &
             "*p1847Y"

;;**************
DEFINE D-MNT-PARM FLOAT     SIZE 8   = PAMMNT OF WCP355B
DEFINE D_MNTLNG   CHARACTER SIZE 1   = &
     FOULNG OF CPFOURNISSEUR IF RECORD CPFOURNISSEUR EXIST &
ELSE "F"

USE usmntfmt NOLIST
;;**************


DEFINE D_FOUEMP   CHARACTER SIZE 12 = "Fournisseur:" IF REFCLETYP = "F" AND D_MNTLNG = "F" &
                                 ELSE "Employ ....:" IF D_MNTLNG = "F" &
                                 ELSE "Supplier...:" IF REFCLETYP = "F" AND D_MNTLNG = "A" &
                                 ELSE "Employe....:"
DEFINE DZ_CIENOM2  CHARACTER SIZE 40  = PARM            ; Nom de la compagnie
DEFINE DZ_CIENOM  CHARACTER SIZE 40  = LJ(DZ_CIENOM2)   ; Nom de la compagnie

DEFINE D_ENT1     CHARACTER SIZE 130 = "#Facture      Date       Montant     Escompte        Net             #Facture      Date       Montant     Escompte        Net"  IF D_MNTLNG = "F" AND T_TYPCHQ = "1"&
                                  ELSE "Invoice#      Date       Amount      Discount        Net             Invoice#      Date       Amount      Discount        Net"  IF T_TYPCHQ = "1" &
                                  ELSE ""

DEFINE D_ENT2     CHARACTER SIZE 130 = "#Facture      Date       Montant     Escompte        Net            #Facture      Date       Montant     Escompte        Net"  IF D_MNTLNG = "F" AND T_TYPCHQ = "1"&
                                  ELSE "Invoice#      Date       Amount      Discount        Net            Invoice#      Date       Amount      Discount        Net"  IF T_TYPCHQ = "1" &
                                  ELSE ""

DEFINE D_CHQ      CHARACTER SIZE 10 = "#Ch que:" IF D_MNTLNG = "F" &
                                 ELSE "Cheque#"

DEFINE  D_EFFFAC  VARCHAR SIZE 71 &
        = PTADSC OF WCP355B + "           " IF T_TYPCHQ = "2" ELSE ""
DEFINE  D_EFFFAC2  VARCHAR SIZE 135 &
        = ""[1:135] IF T_TYPCHQ = "2" ELSE ""
DEFINE  D_EFFDSC  VARCHAR SIZE 60 &
        = ""[1:60] IF T_TYPCHQ = "1" ELSE ""


DEFINE  D_TALON1  VARCHAR SIZE 71 &
        = D-SAUT-TALON1-FAC +  " " + PTADSC OF WCP355B + "           " IF T_TYPCHQ = "2" ELSE ""
DEFINE  D_TALON2  VARCHAR SIZE 71 &
        = D-SAUT-TALON2-FAC +  " " + PTADSC OF WCP355B + "           " IF T_TYPCHQ = "2" ELSE ""

