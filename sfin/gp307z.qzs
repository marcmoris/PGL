USE ussetqzs NOLIST
SET FORMFEED NOBLANKS

ACCESS GPPROJETS &
  LINK CIECLE, PRJCLE TO &
       CIECLE, PRJCLE OF GPPRO_ACTIVITE OPT &
  LINK ("RESCLE", PRJRES OF GPPROJETS) TO &
         XELNOM, CBICLE OF VCBI_DSC OPT &
  LINK ("RESCLE", PRARES OF GPPRO_ACTIVITE) TO &
         XELNOM, CBICLE OF VCBI_DSC ALIAS VCBI_DSC_ACT OPT

CHOOSE CIECLE PARM, &
       PRJCLE PARM

SELECT GPPRO_ACTIVITE IF PRACLE OF GPPRO_ACTIVITE <> "000"

DEFINE DZ_NOMRAP1 CHARACTER SIZE 60 = &   ; titre1  du rapport
