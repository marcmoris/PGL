DEFINE DZ_NUMRAP  CHARACTER SIZE 12 = "GP307"       ; numéro du rapport
DEFINE DZ_CIENOM  CHARACTER SIZE 40 = PARM          ; Nom de la compagnie


USE ustitstd NOLIST
  TAB 001 " " &
  SKIP 2

SORT ON PRJCLE &
     ON PRACLE

