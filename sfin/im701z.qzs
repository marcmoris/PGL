USE ussetqzs NOLIST

;
; Impression des erreurs.
;
ACCESS *WIM701E

;
; Variables obligatoires pour les en-têtes standards.
;
DEFINE DZ_NOMRAP1 CHARACTER SIZE 60 = &
@IF FRANCAIS
       "Transfert des écritures d'aliénations au Grand Livre"
@ELSE
       "%%%Transfert des écritures d'aliénation au Grand Livre"
@ENDIF

DEFINE DZ_NOMRAP2 CHARACTER SIZE 60 = &
@IF FRANCAIS
       "Liste des erreurs"
@ELSE
       "%%%Liste des erreurs"
@ENDIF

;
; Numéro du rapport.
;
DEFINE DZ_NUMRAP  CHARACTER SIZE  7 = "IM701A"
;
; Nom de la compagnie.
;
DEFINE DZ_CIENOM  CHARACTER SIZE 40 = PARM


