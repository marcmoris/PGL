; Ajustement pour le changement de siècle
DEFINE D_ECJCLENUM_TRI CHARACTER SIZE 10 = "1" + ECJCLENUM OF WIM701A & 
   IF ECJCLENUM OF WIM701A[1:1] < "8" &
   ELSE "0" + ECJCLENUM OF WIM701A 

SORT ON D_ECJCLENUM_TRI &
     ON IMMCLE    OF WIM701A


USE ustitstd NOLIST  ; Titre standard
  SKIP 2 &
